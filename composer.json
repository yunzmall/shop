{
  "name": "yun_shop/yun_shop",
  "description": "芸众商城",
  "keywords": [
    "yun_shop yunshop 芸众 商城"
  ],
  "homepage": "http://www.yunzshop.com/",
  "type": "project",
  "license": "Copyrighted",
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.6",
    "ext-bcmath": "*",
    "ext-pcntl": "*",
    "ext-posix": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "laravel/framework": "5.6.*",
    "jeremeamia/superclosure": "~2.2",
    "monolog/monolog": "^1.22",
    "laracasts/flash": "^2.0",
    "printempw/laravel-datatables-lite": "^1.0",
    "iscms/alisms-for-laravel": "^0.0.3",
    "maatwebsite/excel": "~2.1.0",
    "nwidart/db-exporter": "1.*",
    "hao-li/laravel-amount": "dev-master",
    "overtrue/laravel-wechat": "~4.0",
    "watson/bootstrap-form": "^1.1",
    "ixudra/curl": "^6.8",
    "simplesoftwareio/simple-qrcode": "~1",
    "fguillot/json-rpc": "@stable",
    "toplan/laravel-sms": "~2.6",
    "liebig/cron": "1.*",
    "zgldh/laravel-upload-manager": "^0.4.4",
    "vierbergenlars/php-semver": "3.0.*",
    "doctrine/dbal": "v2.5.12",
    "orangehill/iseed": "2.6.1",
    "overtrue/laravel-pinyin": "~3.0",
    "bosnadev/repositories": "0.*",
    "gregwar/captcha": "^1.1",
    "mews/captcha": "^2.2",
    "guzzlehttp/guzzle": "^6.3",
    "phpxmlrpc/phpxmlrpc": "4.0",
    "indigophp/supervisor": "^3.0",
    "rap2hpoutre/laravel-log-viewer": "^0.22.1",
    "predis/predis": "^1.1",
    "wechatpay/wechatpay-guzzle-middleware": "^0.1.1",
    "qcloud/cos-sdk-v5": "^2.0",
    "paypal/rest-api-sdk-php": "1.13.0",
    "workerman/workerman": "^4.0",
    "jenssegers/mongodb": "3.4.*",
    "owen-oj/laravel-getid3": "^1.1",
    "alipaysdk/easysdk": "^2.2"

  },
  "require-dev": {
    "filp/whoops": "^2.1",
    "fzaninotto/faker": "^1.6",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~7.0",
    "barryvdh/laravel-ide-helper": "^2.3",
    "orangehill/iseed": "2.6.1",
    "xethron/migrations-generator": "^2.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "app\\": "app/",
      "App\\": "app/"
    },
    "files": [
      "app/yunshop.php",
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate"
    ]
  },
  "config": {
    "process-timeout": 1800
  },
  "extra": {},
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
