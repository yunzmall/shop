{
  "name": "yun_shop/yun_shop",
  "description": "商城",
  "keywords": [
    "yun_shop yunshop 商城"
  ],
  "type": "project",
  "license": "Copyrighted",
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2",
    "ext-bcmath": "*",
    "ext-pcntl": "*",
    "ext-posix": "*",
    "ext-curl": "*",
    "ext-json": "*",
    "laravel/framework": "6.19.*",
    "jeremeamia/superclosure": "~2.2",
    "monolog/monolog": "^1.22",
    "laracasts/flash": "^3.0",
    "iscms/alisms-for-laravel": "^0.0.3",
    "maatwebsite/excel": "^3.1.0",
    "hao-li/laravel-amount": "dev-master",
    "overtrue/laravel-wechat": "~5.0",
    "watson/bootstrap-form": "^1.1",
    "ixudra/curl": "^6.8",
    "simplesoftwareio/simple-qrcode": "~1",
    "fguillot/json-rpc": "@stable",
    "liebig/cron": "1.*",
    "zgldh/laravel-upload-manager": "^0.6.0",
    "vierbergenlars/php-semver": "3.0.*",
    "doctrine/dbal": "v2.5.12",
    "orangehill/iseed": "^2.6.2",
    "overtrue/laravel-pinyin": "~3.0",
    "gregwar/captcha": "^1.1",
    "mews/captcha": "^3.2",
    "guzzlehttp/guzzle": "^6.3",
    "phpxmlrpc/phpxmlrpc": "4.0",
    "supervisorphp/supervisor": "^3.0",
    "rap2hpoutre/laravel-log-viewer": "^1.3.2",
    "predis/predis": "^1.1",
    "wechatpay/wechatpay-guzzle-middleware": "^0.1.1",
    "qcloud/cos-sdk-v5": "^2.0",
    "paypal/rest-api-sdk-php": "1.13.0",
    "workerman/workerman": "^4.0",
    "owen-oj/laravel-getid3": "^1.1",
    "jenssegers/mongodb": "^3.6.0",
    "alipaysdk/easysdk": "2.2.1",
    "laravel/helpers": "^1.4",
    "tencentcloud/live": "^3.0",
    "tencentcloud/tiia": "^3.0",
    "yansongda/pay": "^2.10",
    "obs/esdk-obs-php": "^3.0"
  },
  "require-dev": {
    "filp/whoops": "^2.1",
    "fzaninotto/faker": "^1.6",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~7.0",
    "barryvdh/laravel-ide-helper": "^2.3"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "app\\": "app/",
      "App\\": "app/",
      "business\\": "business/"
    },
    "files": [
      "app/yunshop.php",
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate"
    ]
  },
  "config": {
    "process-timeout": 1800,
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "easywechat-composer/easywechat-composer": true
    }
  },
  "extra": {},
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}
